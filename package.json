    "cheerio": "^1.0.0-rc.2",
    "dirty-chai": "^2.0.0",
    "eslint": "^8.12.0",
    "eslint-plugin-chai-expect": "^3.0.0",
    "eslint-plugin-mocha": "^10.0.3",
    "mocha": "^9.2.0",
    "phantomjs": "^2.1.7",
    "request": "^2.51.0",
    "saucie": "^3.3.3",
