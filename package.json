  "license": "MIT",
  "dependencies": {
    "async": "^0.9.0",
    "backbone": "^1.1.2",
    "charm": "^1.0.0",
    "colors": "^1.0.3",
    "commander": "^2.6.0",
    "consolidate": "^0.11.0",
    "did_it_work": "0.0.6",
    "express": "^4.10.7",
    "fileset": "^0.1.5",
    "fireworm": "^0.6.6",
    "glob": "^4.3.5",
    "growl": "^1.8.1",
    "http-proxy": "^1.8.1",
    "js-yaml": "^3.2.5",
