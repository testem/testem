    "browser-tests": "cd examples/saucelabs/ && ../../testem.js ci -d",
    "lint": "eslint ."
  },
  "devDependencies": {
    "bluebird-retry": "^0.11.0",
    "browserify": "^17.0.0",
    "chai": "^4.0.2",
