{
    "launchers": {
        "Node": {
            "command": "node tests.js",
            "protocol": "tap",
            "cwd": "tests/fixtures/tape/"
        },
        "NodePlain": {
            "command": "node tests.js",
            "cwd": "tests/fixtures/tape/"
        }
    },
    "src_files": [
        "*.js"
    ],
    "before_tests": "browserify tests.js -o public/bundle.js",
    "test_page": "public/tests.html",
    "launch_in_dev": ["Node", "PhantomJS"]
}